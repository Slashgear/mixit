                    {{/sponsors-hosting}}
                </div>
            {{/has-sponsors-hosting}}
            {{#has-sponsors-video}}
                <div class="mxt-wrapper--callout callout">
                    <h2 class="text-center mxt-subheader--space">{{#i18n}}sponsors.videos{{/i18n}}</h2>
                    {{#sponsors-video}}
                        {{> sponsor }}
                    {{/sponsors-video}}
                </div>
            {{/has-sponsors-video}}
        </div>
    </div>
</div>

{{> footer}}
